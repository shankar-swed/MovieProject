@model CodWizardsMovieShop.Models.ViewModels.MovieCustVm

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="mt-5 text-center">
	@{
		if (TempData["Message"] != null)
		{
			<span class="vbmsg"> @TempData["Message"]  </span>

			TempData.Clear();
		}
	}
    <partial name="_MoviePartial" model="@Model.Movies"></partial>
</div>
<div class="mt-5 text-center">
    <partial name="_CustomerPartial" model="@Model.Customers"></partial>
</div>